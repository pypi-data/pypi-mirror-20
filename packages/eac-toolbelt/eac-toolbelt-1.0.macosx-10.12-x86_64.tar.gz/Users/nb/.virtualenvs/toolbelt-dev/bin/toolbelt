#!/Users/nb/.virtualenvs/toolbelt-dev/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'eac-toolbelt==1.0','console_scripts','toolbelt'
__requires__ = 'eac-toolbelt==1.0'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('eac-toolbelt==1.0', 'console_scripts', 'toolbelt')()
    )
