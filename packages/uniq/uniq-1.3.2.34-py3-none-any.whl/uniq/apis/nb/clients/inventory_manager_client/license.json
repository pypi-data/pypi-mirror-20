                "maxUsageCount",
                "usageCount",
                "eulaStatus",
                "usageCountRemaining",
                "expiredPeriod",
                "deployPending"
            ],
            "properties": {
                "physicalIndex": {
                    "type": "string",
                    "description": "Physical entity index"
                },
                "totalCount": {
                    "description": "Total number of this licensed feature",
                    "type": "integer",
                    "format": "int32"
                },
                "usageCount": {
                    "description": "Number of current usages of this licensed feature",
                    "type": "integer",
                    "format": "int32"
                },
                "type": {
                    "enum": [
                        "DEMO",
                        "EXTENSION",
                        "GRACEPERIOD",
                        "PERMANENT",
                        "PAIDSUBSCRIPTION",
                        "EVALUATIONSUBSCRIPTION",
                        "EXTENSIONSUBSCRIPTION",
                        "EVALRIGHTTOUSE",
                        "RIGHTTOUSE",
                        "PERMANENTRIGHTTOUSE"
                    ],
                    "type": "string",
                    "description": "Type of license based on the validity period"
                },
                "validityPeriodRemaining": {
                    "description": "Time period remaining before the license expires or transitions to rightToUse(9) license. Value will be in milliseconds",
                    "type": "integer",
                    "format": "int64"
                },
                "expiredDate": {
                    "type": "string",
                    "description": "Expired date of the license"
                },
                "isEulaApplicable": {
                    "type": "boolean",
                    "description": "This field is based on eulaStatus. Ex: If eulaStatus is true then it will be applicable else false"
                },
                "isCounted": {
                    "type": "boolean",
                    "description": "Whether the license is counted license. Values are true(1)  - counted license,false(2) - uncounted license"
                },
                "attributeInfo": {
                    "$ref": "object"
                },
                "eulaStatus": {
                    "type": "boolean",
                    "description": "Whether the user accepted end user license agreement for this license. Values are true(1)  - EULA accepted, false(2) - EULA not accepted"
                },
                "validityPeriod": {
                    "description": "Time period the license is valid for. Value will be in milliseconds",
                    "type": "integer",
                    "format": "int64"
                },
                "evalPeriodUsed": {
                    "type": "string",
                    "description": "Number of days used in the eval period"
                },
                "expiredPeriod": {
                    "description": "Time period after the license expires. Value will be in milliseconds",
                    "type": "integer",
                    "format": "int32"
                },
                "id": {
                    "type": "string",
                    "description": "Id of the license"
                },
                "licenseIndex": {
                    "description": "Index of the license to uniquely identify a license within the device",
                    "type": "integer",
                    "format": "int32"
                },
                "storeName": {
                    "type": "string",
                    "description": "Name of the license store within the device. Ex: 'disk1:lic_store_1.txt' or  'flash:lic_store_2.txt"
                },
                "isTechnologyLicense": {
                    "type": "boolean",
                    "description": "Whether the license is technology license. Values are true(1)  - technology license,false(2) - nontechnology license"
                },
                "hostId": {
                    "type": "string",
                    "description": "An administratively-assigned fully-qualified domain name for this managed node"
                },
                "storedUsed": {
                    "description": "License store that is used for storing this license",
                    "type": "integer",
                    "format": "int32"
                },
                "description": {
                    "type": "string",
                    "description": "Description about the license. It is populated  with comments from the license file"
                },
                "usageCountRemaining": {
                    "description": "Number of entities that can still use this license",
                    "type": "integer",
                    "format": "int64"
                },
                "parentId": {
                    "description": "Parent Id of the license",
                    "type": "integer",
                    "format": "int32"
                },
                "status": {
                    "enum": [
                        "INACTIVE",
                        "NOTINUSE",
                        "INUSE",
                        "EXPIREDINUSE",
                        "EXPIREDNOTINUSE",
                        "USAGECOUNTCONSUMED"
                    ],
                    "type": "string",
                    "description": "Status of the license"
                },
                "provisionState": {
                    "description": "Provision state of the license feature",
                    "type": "integer",
                    "format": "int32"
                },
                "licenseFileCount": {
                    "description": "Number of installed license file in this feature",
                    "type": "integer",
                    "format": "int32"
                },
                "priority": {
                    "type": "string",
                    "description": "License priority"
                },
                "name": {
                    "type": "string",
                    "description": "Name of the feature that is using or can use this license. Ex: 'IPBASE', 'ADVIPSERVICE'"
                },
                "isEulaAccepted": {
                    "type": "boolean",
                    "description": "This field is based on eulaStatus. Ex: If eulaStatus is true then it will be accepted else false"
                },
                "featureVersion": {
                    "type": "string",
                    "description": "Version of the feature that is using or can use this license. Ex: '1.0', '2.0' "
                },
