        {
            "Sid": "BrowseBucket",
            "Effect": "Allow",
            "Action": [
                "s3:ListBucket"
            ],
            "Resource": [
                {% for bucket in items -%}
                  "arn:aws:s3:::{{ bucket }}",
                {% endfor -%}
                "arn:aws:s3:::default_bucket_name"
            ]
        },
        {
            "Effect": "Allow",
            "Action": [
                "s3:DeleteObject",
                "s3:GetObject",
                "s3:PutObject"
            ],
            "Resource": [
                {% for bucket in items -%}
                  "arn:aws:s3:::{{ bucket }}/*",
                {% endfor -%}
                "arn:aws:s3:::default_bucket_name/*"
            ]
        }
