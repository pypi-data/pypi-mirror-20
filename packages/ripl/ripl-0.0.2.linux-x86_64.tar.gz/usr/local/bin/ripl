#!/usr/local/bin/python3.6
# EASY-INSTALL-ENTRY-SCRIPT: 'ripl==0.0.2','console_scripts','ripl'
__requires__ = 'ripl==0.0.2'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('ripl==0.0.2', 'console_scripts', 'ripl')()
    )
