#!/usr/bin/env python
# -*- coding: utf-8 -*-

import sys
import os

scriptdir = os.path.abspath(os.path.dirname(sys.argv[0]))
assert sys.path[0] == scriptdir
sys.path[0] = os.path.normpath(os.path.join(scriptdir, os.pardir))

from atofido.app import Application
from atofido.config.parser import Parser


def main():
    Application(Parser()).start(sys.argv[1:])


if __name__ == "__main__":
    main()
