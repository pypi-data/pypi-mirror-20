{
    "$schema": "http://json-schema.org/schema#",
    "additionalProperties": false,
    "properties": {
        "date": {
            "type": "string"
        },
        "email": {
            "format": "email",
            "type": "string"
        },
        "internal_uid": {
            "type": "integer"
        },
        "method": {
            "enum": [
                "submitter",
                "oai",
                "batchuploader",
                "hepcrawl"
            ],
            "type": "string"
        },
        "orcid": {
            "pattern": "^\\d{4}-\\d{4}-\\d{4}-\\d{3}[0-9X]$",
            "type": "string"
        },
        "source": {
            "$ref": "source.json"
        },
        "submission_number": {
            "type": "string"
        }
    },
    "type": "object"
}
